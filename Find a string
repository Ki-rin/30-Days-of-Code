def count_substring(string, sub_string):
    arr = [string[i:i+len(sub_string)] for i in range(len(string))]
    #print(arr)
    answ = arr.count(sub_string)
    return answ

if __name__ == '__main__':
    string = input().strip()
    sub_string = input().strip()
    
    count = count_substring(string, sub_string)
    print(count)
