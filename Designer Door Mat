m = '-'
k = '.|.'
w = 'WELCOME'

h, l = list(map(int,input().split()))

#Top Cone
for i in range((h-1)//2):
    print((m*((l-(i*2+1)*3)//2))+k*(2*i+1)+(m*((l-(i*2+1)*3)//2)))


#Middle Belt
print((m*((l-7)//2))+w+(m*((l-7)//2)))



#Bottom Cone
for i in range(h//2-1,-1,-1):
    print((m*((l-(i*2+1)*3)//2))+k*(2*i+1)+(m*((l-(i*2+1)*3)//2)))
